        <Button Content="Login" HorizontalAlignment="Left" Margin="118,116,0,0" VerticalAlignment="Top" Width="76" Click="Button_Click"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="34,22,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="34,68,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="textbox2" HorizontalAlignment="Left" Margin="100,68,0,0" VerticalAlignment="Top" Width="120" Height="24"/>

    </Grid>
